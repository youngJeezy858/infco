<%= form_for(@reloud_check) do |f| %>
<% if @reloud_check.errors.any? %>
<div id="error_explanation">
  <h2><%= pluralize(@reloud_check.errors.count, "error") %> prohibited this reloud_check from being saved:</h2>
  
  <ul>
    <% @reloud_check.errors.full_messages.each do |msg| %>
